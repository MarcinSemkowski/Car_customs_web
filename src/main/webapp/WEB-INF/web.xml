<web-app
        xmlns="http://java.sun.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
        version="3.0">
    

    <servlet>
        <servlet-name>sing_up</servlet-name>
        <servlet-class>Servlets.Sing_up_Servlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>sing_up</servlet-name>
        <url-pattern>/sing_up.do</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>Servlets.logout_Servlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>log</servlet-name>
        <servlet-class>Servlets.log_in_Servlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>log</servlet-name>
        <url-pattern>/login.do</url-pattern>
    </servlet-mapping>


</web-app>